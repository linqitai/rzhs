                        <div class="priceBox right">
                            <div class="goodsPrice">&yen {{price}}</div>
                            <div class="voucherBox">使用代金券<span class="voucher">{{discount}}</span>元</div>
                        </div>
                    </a>
                    <div class="btnGroup">
                        <button class="btn btn-default right cancelBtn confirm-ok-cancel" data-id="{{id}}">取消订单</button>
                    </div>
                </div>
            </div>
        </div>
    {{/ifCond}}
    {{#ifCond status "==" 1}}
        <div class="orderList">
            <div class="storeName">{{storeName}}</div>
            <div class="goodsInfoItem">
                <a href="/product/goodDetails/goodsDetails.htm?id={{productId}}" external>
