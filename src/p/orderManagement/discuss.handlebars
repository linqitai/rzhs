{{#each this}}
    <div class="orderList">
        <div class="storeName">{{storeName}}</div>
        <div class="goodsInfoItem">
            <a href="/product/goodDetails/goodsDetails.htm?id={{productId}}" external>
                <img class="goodsPic left" src="{{imgUrl}}">
            </a>
            <div class="rightBox">
                <a href="/product/goodDetails/goodsDetails.htm?id={{productId}}" class="goodsInfo" external>
                    <div class="goodsName">{{name}}</div>
                    <div class="priceBox right">
                        <div class="goodsPrice">&yen {{price}}</div>
                        <div class="voucherBox">使用代金券<span class="voucher">{{discount}}</span>元</div>
                    </div>
                </a>
                <div class="btnGroup">
                    <button class="btn btn-default right not-fill discussBtn" data-id="{{id}}" data-productId="{{productId}}">评价</button>
                </div>
            </div>
        </div>
    </div>
{{/each}}

